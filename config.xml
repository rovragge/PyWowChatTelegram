<config>
    <logger>
        <name>LOGGER</name>
        <level>DEBUG</level>
        <to_file>0</to_file>
        <to_stdout>1</to_stdout>
        <format>%(levelname)s: %(message)s</format>
    </logger>
    <telegram>
        <db>https://wowroad.info/</db>
        <channels>
            <say>say-channel</say>
            <guild>guild-chat</guild>
            <whisper>whisper-channel</whisper>
            <system>system-channel</system>
            <announcement>rt-channel</announcement>
            <officer>officer-channel</officer>
            <yell>yell-channel</yell>
        </channels>
    </telegram>
    <wow>
        <reconnect_delay>10</reconnect_delay>
        <platform>Win</platform>
        <locale>enUS</locale>
        <server_motd_enabled>1</server_motd_enabled>
        <version>3.3.5</version>
    </wow>
    <guild_events>
        <signed_on>1</signed_on>
        <signed_off>1</signed_off>
        <promoted>1</promoted>
        <demoted>1</demoted>
        <joined>1</joined>
        <left>1</left>
        <removed>1</removed>
        <motd>1</motd>
    </guild_events>
    <achievements>1</achievements>
</config>